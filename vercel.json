{
    "name": "mi-proyecto",
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "index.js" 
      },
      {
        "src": "/api-doc/(.*)",
        "dest": "index.js" 
      },
      {
        "src": "/(.*)",
        "dest": "/" 
      }
    ]
  }